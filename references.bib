@book{organizacao_projeto,
	title = {Organização e Projeto de Computadores},
	copyright = {Book},
	isbn = {9788535235852},
	abstract = {Este livro apresenta conceitos que são a base para os computadores atuais, destacando os tópicos de interface de hardware e software. Com foco na mudança revolucionária de computação sequencial para paralela, Organização e Projeto de Computadores aborda o surgimento e a importância da GPU moderna, descrevendo em detalhes o multicore microprocessador, altamente otimizado para vários segmentos da computação visual.
    Além do enfoque em paralelismo oferece conteúdos em Flash de memória e máquinas virtuais, bem como um conjunto completo de exercícios sugeridos por professores e alunos.
    Os temas abordados, bem como os exercícios presentes no final da cada capítulo, tornam este livro adequado tanto para aqueles com pouca experiência na área, mas que precisam entender a organização básica do computador, quanto para aqueles que já possuem uma base em linguagem assembly e projeto lógico e desejam aprender a projetar um computador ou entender como e por que um sistema funciona de determinada forma.},
	language = {pt},
	note = {Book},
	publisher = {Campus - Grupo Elsevier},
	author = {Patterson, David A.},
	year = {2009},
	keywords = {Computers / Computer Science, Computers / General, Science / General}
}

